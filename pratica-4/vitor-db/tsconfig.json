{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "module": "esnext",
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "rootDir": "./",
    "baseUrl": "./",
    "paths": {"app/*": ["./src/app/*"],
              "cp/*": ["./src/cp/*"],
              "navigation/*": ["src/navigation/*"],
              "screen/*": ["./src/screen/*"],
              "db/*": ["./src/db/*"],
              },
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strictPropertyInitialization": false,                 
  }
  
}